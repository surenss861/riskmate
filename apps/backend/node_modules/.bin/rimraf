#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/surensureshkumar/coding projects/riskmate/apps/backend/node_modules/rimraf/node_modules:/Users/surensureshkumar/coding projects/riskmate/apps/backend/node_modules:/Users/surensureshkumar/coding projects/riskmate/apps/node_modules:/Users/surensureshkumar/coding projects/riskmate/node_modules:/Users/surensureshkumar/coding projects/node_modules:/Users/surensureshkumar/node_modules:/Users/node_modules:/node_modules:/Users/surensureshkumar/coding projects/riskmate/apps/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/surensureshkumar/coding projects/riskmate/apps/backend/node_modules/rimraf/node_modules:/Users/surensureshkumar/coding projects/riskmate/apps/backend/node_modules:/Users/surensureshkumar/coding projects/riskmate/apps/node_modules:/Users/surensureshkumar/coding projects/riskmate/node_modules:/Users/surensureshkumar/coding projects/node_modules:/Users/surensureshkumar/node_modules:/Users/node_modules:/node_modules:/Users/surensureshkumar/coding projects/riskmate/apps/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rimraf/bin.js" "$@"
else
  exec node  "$basedir/../rimraf/bin.js" "$@"
fi
